
{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "client": {
      "android": {
        "buildType": "apk",
        "env": {
          "APP_MODE": "client"
        }
      },
      "ios": {
        "buildType": "archive",
        "env": {
          "APP_MODE": "client"
        }
      }
    },
    "owner": {
      "android": {
        "buildType": "apk",
        "env": {
          "APP_MODE": "owner"
        }
      },
      "ios": {
        "buildType": "archive",
        "env": {
          "APP_MODE": "owner"
        }
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {}
  }
}
